#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules/mkdirp/bin/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules/mkdirp/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/node_modules:/workspaces/node_modules:/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules/mkdirp/bin/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules/mkdirp/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/node_modules:/workspaces/node_modules:/node_modules:/workspaces/complex-techniques-rja-2024-aidan-george.worktrees/Nest-Web-Framework-test/trial/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
